<%= bootstrap_form_for @offer do |f| %>
  <%= f.hidden_field :user_id %>
  <%= f.text_field :title %>
  <%= f.text_area :description, rows: 5 %>
  <%= render partial: 'shared/address_inputs', locals: {form: f} %>
  <%= f.form_group do %>
    <%= f.primary class: 'btn btn-primary' %>
  <% end %>
<% end %>

